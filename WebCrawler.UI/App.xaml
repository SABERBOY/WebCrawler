<Application x:Class="WebCrawler.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:cvt="clr-namespace:WebCrawler.UI.Converters"
             xmlns:local="clr-namespace:WebCrawler.UI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Themes/Icons.xaml"/>
                <ResourceDictionary Source="/Themes/Default.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <cvt:BinaryConverter x:Key="BinaryConverter" />
            <cvt:MatchEnumConverter x:Key="MatchEnumConverter" />
            <!--<cvt:MatchStringConverter x:Key="MatchStringConverter" />-->
            <!--<cvt:EmptyStringConverter x:Key="EmptyStringConverter" />-->
            <!--<cvt:EmptyNumberConverter x:Key="EmptyNumberConverter" />-->
            <cvt:EmptyObjectConverter x:Key="EmptyObjectConverter" />
            <cvt:EmptyCollectionConverter x:Key="EmptyCollectionConverter" />

            <cvt:EnumDescriptionConverter x:Key="EnumDescriptionConverter" />

        </ResourceDictionary>
    </Application.Resources>
</Application>
